<div class="spacer"></div>

<p id="notice"><%= notice %></p>

<h1>Dev Corners</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Skills</th>
      <th>Experience</th>
      <th>Strengths</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dev_corners.each do |dev_corner| %>
      <tr>
        <td><%= dev_corner.profile %></td>
        <td><%= dev_corner.skills %></td>
        <td><%= dev_corner.experience %></td>
        <td><%= dev_corner.strengths %></td>
        <td><%= link_to 'Show', dev_corner %></td>
        <td><%= link_to 'Edit', edit_dev_corner_path(dev_corner) %></td>
        <td><%= link_to 'Destroy', dev_corner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dev Corner', new_dev_corner_path %>
